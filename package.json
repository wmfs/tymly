{
  "name": "tymly",
  "description": "An empty husk for synchronizing Tymly related packages, plugins, blueprints etc.",
  "version": "0.0.0",
  "author": "Tim Needham",
  "devDependencies": {
    "lerna": "3.6.0",
    "mocha": "5.2.0",
    "chai": "4.2.0"
  },
  "scripts": {
    "bootstrap": "lerna clean --yes && lerna bootstrap --nohoist='@wmfs/*' && lerna link --force-local",
    "clean": "lerna clean --yes",
    "test": "lerna run test",
    "sync": "node ./lib/sync"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/tymly"
  },
  "bugs": {
    "url": "https://github.com/wmfs/tymly/issues"
  },
  "dependencies": {
    "@octokit/rest": "16.2.0",
    "chalk": "2.4.1",
    "config": "3.0.1",
    "glob": "7.1.3",
    "helmet": "3.15.0",
    "mem-fs": "1.1.3",
    "mem-fs-editor": "5.1.0",
    "moment": "2.23.0",
    "npm-registry-client": "8.6.0",
    "opn": "5.4.0",
    "prompt": "1.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
