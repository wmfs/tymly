{
  "name": "tymly-monorepo",
  "description": "An empty husk for adding Tymly related packages, plugins, blueprints etc.",
  "version": "0.0.0",
  "author": "Tim Needham",
  "devDependencies": {
    "lerna": "2.11.0",
    "mocha": "5.2.0",
    "chai": "4.2.0"
  },
  "scripts": {
    "bootstrap": "lerna clean --yes && lerna bootstrap --nohoist='@wmfs/*' && lerna link --force-local",
    "clean": "lerna clean --yes",
    "test": "lerna run test",
    "sync": "node ./lib"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/tymly-monorepo"
  },
  "bugs": {
    "url": "https://github.com/wmfs/tymly-monorepo/issues"
  },
  "dependencies": {
    "@octokit/rest": "15.8.1",
    "axios": "0.18.0",
    "body-parser": "1.18.3",
    "config": "2.0.1",
    "chalk": "2.4.1",
    "ejs": "2.6.1",
    "express": "4.16.4",
    "glob": "7.1.3",
    "helmet": "3.15.0",
    "mem-fs": "1.1.3",
    "mem-fs-editor": "4.0.0",
    "moment": "2.22.2",
    "npm-registry-client": "8.5.1",
    "opn": "5.3.0",
    "prompt": "1.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
