{
  "Comment": "Workflow to remove a website post",
  "version": "1.0",
  "instigators": [
    "user"
  ],
  "tags": [
    "website",
    "posts"
  ],
  "StartAt": "FormFilling",
  "States": {
    "FormFilling": {
      "Type": "Task",
      "Resource": "module:formFilling",
      "ResourceConfig": {
        "formId": "deletePostForm"
      },
      "End": true
    }
  }
}
