{
  "boardTitleTemplate": "Incident ${incidentNumber}/${incidentYear}",
  "watchable": true,
  "category": "incidents",
  "content": [
    {
      "widget": "summary",
      "config": {
        "template": "We can show some HTML stuff here"
      }
    },
    {
      "widget": "maplist",
      "config": {
        "header": "Map",
        "map": {
          "latProperty": "lat",
          "lonProperty": "lon",
          "editable": false,
          "drawing": false,
          "layers": [
            "hydrants"
          ],
          "3d": false,
          "initialZoom": "4"
        },
        "properties": [
          {
            "prompt": "Over border",
            "dataPath": "overBorder",
            "showIfUndefined": true
          },
          {
            "prompt": "Station area",
            "dataPath": "stationArea",
            "showIfUndefined": true
          },
          {
            "prompt": "Station responsible",
            "dataPath": "stationResponsible",
            "showIfUndefined": true
          },
          {
            "prompt": "Station call",
            "dataPath": "stationCall",
            "showIfUndefined": true
          },
          {
            "prompt": "Position latitude",
            "dataPath": "lat",
            "showIfUndefined": true
          },
          {
            "prompt": "Position longitude",
            "dataPath": "long",
            "showIfUndefined": true
          },
          {
            "prompt": "Address",
            "dataPath": "addressLines",
            "showIfUndefined": true
          }
        ]
      }
    },
    {
      "widget": "propertylist",
      "config": {
        "header": "Summary",
        "properties": [
          {
            "prompt": "Resource usage",
            "dataPath": "resourceSummary.byCallsignType",
            "showIfUndefined": true
          },
          {
            "prompt": "Attendance summary",
            "dataPath": "resourceSummary.byAttendance",
            "showIfUndefined": true
          },
          {
            "prompt": "First Mobilised",
            "dataPath": "resourceSummary.firstMobilised",
            "showIfUndefined": true
          },
          {
            "prompt": "First in Attendance",
            "dataPath": "resourceSummary.firstInAttendance",
            "showIfUndefined": true
          },
          {
            "prompt": "First Stop",
            "dataPath": "resourceSummary.firstStop",
            "showIfUndefined": true
          },
          {
            "prompt": "Last Appliance Returned",
            "dataPath": "resourceSummary.lastApplianceReturned",
            "showIfUndefined": true
          },
          {
            "prompt": "Reaction Time",
            "dataPath": "resourceSummary.reactionTime",
            "showIfUndefined": true
          },
          {
            "prompt": "Response Time",
            "dataPath": "resourceSummary.responseTime",
            "showIfUndefined": true
          }
        ]
      }
    },
    {
      "widget": "tabularlist",
      "config": {
        "header": "Individual Resources",
        "dataPath": "callsigns",
        "columns": [
          {
            "header": "Callsign",
            "dataPath": "callsign"
          },
          {
            "header": "Type",
            "dataPath": "type"
          },
          {
            "header": "Crew Count",
            "dataPath": "crewCount"
          },
          {
            "header": "Assigned",
            "dataPath": "assigned"
          },
          {
            "header": "Mobilised",
            "dataPath": "mobilised"
          },
          {
            "header": "Attended",
            "dataPath": "attended"
          },
          {
            "header": "Stop",
            "dataPath": "stop"
          },
          {
            "header": "Released",
            "dataPath": "released"
          }
        ]
      }
    }
  ]
}